<%= form_with scope: :course, url: courses_path, local: true do |form| %>
    <p>
      <%= form.label :course_id %><br>
      <%= form.text_field :course_id %>
    </p>
   
    <p>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
    </p>

    <p>
      <%= form.label :subject %><br>
      <%= form.text_area :subject %>
    </p>


    <p>
      <%= form.label :term %><br>
      <%= form.text_field :term %>
    </p>

    <p>
      <%= form.label :course_code %><br>
      <%= form.text_field :course_code %>
    </p>

    <p>
      <%= form.label :students %><br>
      <%= form.text_field :students %>
    </p>

    <p>
      <%= form.label :notes %><br>
      <%= form.text_field :notes %>
    </p>

    <p>
      <%= form.label :links %><br>
      <%= form.text_field :links %>
    </p>

    <p>
      <%= form.label :discussions %><br>
      <%= form.text_field :discussions %>
    </p>

    <p>
      <%= form.label :groups %><br>
      <%= form.text_field :groups %>
    </p>
     
    <p>
      <%= form.submit %>
    </p>
  <% end %>

